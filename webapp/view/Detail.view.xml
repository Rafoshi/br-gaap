<mvc:View
    controllerName="sap.ui.demo.walkthrough.controller.Detail"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page
        showNavButton="true"
        navButtonPress=".onNavBack">
        <ObjectHeader 
            title="ID: {invoice>id}  "
            >
            <ObjectAttribute text="UserID: {invoice>userId}" />
            <ObjectAttribute text="Title: {invoice>title}" />
            <ObjectAttribute text="Completed:  {invoice>completed}" />
        </ObjectHeader>
    </Page>
</mvc:View>